@prefix fno: <https://w3id.org/function/ontology#> .
@prefix fnoc: <https://w3id.org/function/vocabulary/composition#> .
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#> .
@prefix fnoi: <https://w3id.org/function/vocabulary/implementation#> .
@prefix fns: <http://example.com/functions#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

fns:ETL
    a fno:Function ;
    dcterms:description "ETL 001"@en ;
    fno:expects ( fns:str0Parameter ) ;
    fno:returns ( fns:strOutput )
.


fns:ETLComposition
    a fnoc:Composition;
    #
    fnoc:composedOf [
        fnoc:mapFrom [
            fnoc:constituentFunction fns:ETL;
            fnoc:functionParameter fns:str0Parameter 
        ] ;
        fnoc:mapTo [
            fnoc:constituentFunction fns:executeRMLMapper;
            fnoc:functionParameter fns:str0Parameter
        ] 
    ],
    [
        fnoc:mapFrom [
            fnoc:constituentFunction fns:executeRMLMapper;
            fnoc:functionOutput fns:strOutput 
        ] ;
        fnoc:mapTo [
            fnoc:constituentFunction fns:publish;
            fnoc:functionParameter fns:str0Parameter
        ] 
    ],
    [
        fnoc:mapFrom [
            fnoc:constituentFunction fns:publish;
            fnoc:functionOutput fns:strOutput 
        ] ;
        fnoc:mapTo [
            fnoc:constituentFunction fns:ETL;
            fnoc:functionOutput fns:strOutput
        ] 
    ]
.